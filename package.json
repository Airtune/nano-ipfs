    "@types/bs58": "^4.0.1",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.3.3",
    "chai": "^4.3.4",
    "documentation": "^13.2.5",
    "mocha": "^9.0.1",
    "nanocurrency": "^2.5.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
